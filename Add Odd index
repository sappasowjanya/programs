//For this challenge, you will be given an array and you are asked to find sum of array elements present at odd index.


#include<stdio.h>
int main(int argc, char *a[])
{
	//Write code here
	int n;
	scanf("%d",&n);
	int i;
	int b[n];
	for(i=0;i<n;i++)
	{
	    scanf("%d",&b[i]);
	}
	int sum=0;
	for(i=0;i<n;i++)
	{
	    if(i%2!=0)
	    {
	        sum=sum+b[i];
	    }
	}
	printf("%d",sum);
	return 0;
}
