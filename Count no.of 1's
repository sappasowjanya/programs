//Given a binary array sorted in non-increasing order, count the number of 1's in it. 

#include<stdio.h>
int main(int argc, char *a[])
{
	//Write code here
	int n;
	scanf("%d",&n);
	int i,b[n],c=0;
	for(i=0;i<n;i++)
	{
	    scanf("%d",&b[i]);
	    if(b[i]==1)
	    {
	        c++;
	    }
	}
	printf("%d",c);
	return 0;
}
