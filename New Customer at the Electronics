#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int n;
    scanf("%d",&n);
    int i,a[n],j;
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    j=n-1;
    for(i=n-2;i>=0;i--)
    {
        
        if(a[i]>a[j])
        {
            int temp=a[i];
            a[i]=a[j];
            a[j]=temp;
            j--;
        }
        else
        {
            break;
        }
    }
    for(i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
    return 0;
}
