#include<stdio.h>
int main(int argc, char *a[])
{
	//Write code here
	  int n,n1;
	  scanf("%d %d",&n,&n1);
	  int b[n][n1];
	  int i,j,sum=0;
	  for(i=0;i<n;i++)
	   {
	       for(j=0;j<n1;j++)
	       {
	           scanf("%d",&b[i][j]);
	           if(b[i][j]%2==0)
	           {
	               printf("%d ",b[i][j]);
	           }
	       }
	   }
	   return 0;
}
