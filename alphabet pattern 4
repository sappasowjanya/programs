#include<stdio.h>
int main(int argc, char *a[])
{
	//Write code here
	int num;
scanf("%d",&num);
int i,j,k;
for(i=1;i<=num;i++)
{
    for(j=1;j<i;j++)
    {
        printf("  ");
    }
    for(k=i;k<=num;k++)
    {
        printf("%c",65+num-i);
        if(k!=num)
        printf(" ");
    }
    if(i!=num)
    printf("\n");
}
}
