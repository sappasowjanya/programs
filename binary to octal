#include <stdio.h>
 
int main(void) {
 long int binary, octal = 0, j = 1, rem;
    scanf("%ld", &binary);
    while (binary != 0)
    {
        rem = binary % 10;
        octal = octal + rem * j;
        j = j * 2;
        binary= binary/ 10;
    }
    printf("%o", octalnum);//%o is for octal ,%d is for decimal
