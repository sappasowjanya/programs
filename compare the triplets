#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
    int i,a[100],b[100],c=0,k=0;
    for(i=0;i<3;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int r=0;r<3;r++)
    {
        scanf("%d",&b[r]);
    }
    for(i=0;i<3;i++)
    {
        if(a[i]>b[i])
        {
         c++;
            
        }
       if(a[i]<b[i])
       {
           k++;
       }
    }
    printf("%d %d",c,k);
    return 0;
}
