/*For this challenge, you will be given an array and you are asked to count elements which are divisible by either 3, 5 or both. */












#include<stdio.h>
int main(int argc, char *a[])
{
	//Write code here
	int n;
	scanf("%d",&n);
	int b[n],i,count=0;
	for(i=0;i<n;i++)
	{
	    scanf("%d",&b[i]);
	    if(b[i]%3==0||b[i]%5==0)
	    {
	        count++;
	    }
	}
	printf("%d",count);
	return 0;
}
