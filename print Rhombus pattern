#include <stdio.h>
 
int main(void) {
	// your code goes here
       int n;
      scanf("%d",&n);
      int i,j;
      for(i=0;i<n;i++)
      {
        for(j=0;j<i;j++)
        {
           printf(" ");
        }
        for(j=0;j<n;j++)
         {
           printf("*");
        }
      printf("\n");
     }
	return 0;
}
